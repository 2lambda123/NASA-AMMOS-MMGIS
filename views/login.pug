doctype html
head
  title MMGIS / Config
  link(rel='shortcut icon' href='resources/logo.png')
  link(type='text/css' rel='stylesheet' href='config/css/login.css')
  script(type='text/javascript' src='config/js/jquery.min.js')
  script(type='text/javascript' src='config/js/login.js')
body.mmgisScrollbar
  .animated.bounceInDown
  #setup
    | SETUP
  .container
    img(src='resources/mmgis.png' alt='MMGIS' onClick='back()')
    span#msg.error.animated.tada
    form.box(name='form1')
      h4
        | Admin
        span Dashboard
      h5 Sign in to your account.
      input#username(type='text' name='username' placeholder='Username' autocomplete='off')
      i#eye.typcn.typcn-eye
      input#pwd(type='password' name='password' placeholder='Password' autocomplete='off')
      input#pwd_retype(type='password' name='password' placeholder='Retype Password' autocomplete='off')
      div.btn1(onClick='login()') Sign in